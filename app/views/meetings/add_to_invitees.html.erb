<div id="add_to_invitees_list">
    <% form_tag( :controller => 'meetings', :action => :update_invitees, :id => @meeting ) do %>
        <%= submit_tag 'Add Selected' %>
        <p class="select_list">&nbsp;</p>
        <% for committee_role in @committee_roles %>
            <p class="select_list"> <%= check_box_tag("committee_role_ids[]",committee_role.id) %> <%= committee_role.sname %> </p>
        <% end %>
        <p class="select_list">&nbsp;</p>
        <%= submit_tag 'Add Selected' %>
    <% end %> <!-- end form_tag -->
</div>

