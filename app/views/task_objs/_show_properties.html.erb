<% end %>
<% } %>


<% @ft_view_section[:month] = capture { %>
<% if @show_item[:month] then %>

    <!-- Month -->

    <div class="show_attr_group attr_group_task_obj_tasks">     <!-- div16b -->
      <div class="show_attr_label"> Month </div>         <!-- div17b -->
      <div class="show_attr_value"> <%= @task_obj.month.nil? ? "" : link_to(@task_obj.month.iname, url_show(@task_obj.month)) %>&nbsp;</div>
    </div>         <!-- end div16b -->

<% end %>
<% } %>

