<div  id="add_new_to_announcements" style="float:right; width:60%;">
    <% dest = url_for :controller => 'task_objs', :action => 'update_announcements', :id => @task_obj.id %>
    <% form_tag( :controller => 'announcements', :action => 'new', :redirect => dest ) do %>
        <%= submit_tag 'Add New...' %>
    <% end %> <!-- end form_tag -->
</div>

<div id="add_to_announcements_list">
    <% form_tag( :controller => 'task_objs', :action => :update_announcements, :id => @task_obj ) do %>
        <%= submit_tag 'Add Selected' %>
        <p class="select_list">&nbsp;</p>
        <% for announcement in @announcements %>
            <p class="select_list"> <%= check_box_tag("announcement_ids[]",announcement.id) %> <%= announcement.sname %> </p>
        <% end %>
        <p class="select_list">&nbsp;</p>
        <%= submit_tag 'Add Selected' %>
    <% end %> <!-- end form_tag -->
</div>

